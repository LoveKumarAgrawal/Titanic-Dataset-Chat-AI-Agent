{
    "version": 2,
    "builds": [
        {
            "src": "./backend.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/{.*}",
            "dest": "./backend.py"
        }
    ],
    "excludeFiles": [
    "**/*.pyc",
    "**/__pycache__/**",
    ".env",
    ".git/**",
    ".venv/**",
    "automations/**",
    "docs/**",
    "logdir/**",
    "tests/**",
    "prisma/migrations/**",
    "Dockerfile",
    "README.md",
    "pytest.ini",
    "package.json"
  ]
}